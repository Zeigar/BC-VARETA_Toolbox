<?xml version="1.0" encoding="utf-8"?>
<properties>
    
    <bs_path>D:\NeuroInformatics Lab\BC-VARETA1.0_repo\brainstorm3</bs_path>
    <bs_url>https://codeload.github.com/brainstorm-tools/brainstorm3/zip/master</bs_url>
    <bs_db_path>C:\Users\Ariosky\.brainstorm\local_db</bs_db_path>
    <raw_data_path>D:\NeuroInformatics Lab\LeadField-Pipeline-BS\data</raw_data_path>
    <protocol_name>ExampleProtocol</protocol_name> 
    
       
    <process_import_anatomy>
        <mrifile2>FreeSurfer</mrifile2>
        <nvertices>6001</nvertices>                    
        <aseg>1</aseg>
    </process_import_anatomy>
    
    <process_generate_bem>
        <nscalp>1922</nscalp>
        <nouter>1922</nouter>
        <ninner>1922</ninner>
        <thickness>4</thickness>
    </process_generate_bem> 
    
    <process_import_data_raw>
        <datafile>EEG-MAT</datafile>
        <channelreplace>1</channelreplace>
        <channelalign>1</channelalign>
        <evtmode>value</evtmode>
    </process_import_data_raw>
    
    <process_import_channel>
        <usedefault>43</usedefault>
        <channelalign>1</channelalign>
        <fixunits>1</fixunits>
        <vox2ras>1</vox2ras>
    </process_import_channel>
    
    <process_headmodel>
        <Comment/>
        <sourcespace>1</sourcespace>
        
        <Method>isotropic</Method>
        <nLayers>17</nLayers>
        <Reduction>3</Reduction>
        <nVerticesInit>4000</nVerticesInit>
        <Resolution>0.005</Resolution>
        <FileName/>
        
        <eeg>3</eeg>
        
        <BemSelect>1,1,12</BemSelect>
        <BemCond>1,0.0125,1</BemCond>
        <BemNames>Scalp,Skull,Brain</BemNames>
        <BemFiles/>
